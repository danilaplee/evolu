{
  "cli": {
    "version": ">= 10.0.2"
  },
  "build": {
    "test": {
      "android": {
        "gradleCommand": ":app:assembleRelease :app:assembleAndroidTest -DtestBuildType=release",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": true
      },
      "buildArtifactPaths": ["artifacts/**/*.png"]
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "config":"pnpm.yml"
    },
    "preview": {
      "distribution": "internal",
      "config":"pnpm.yml"
    },
    "production": {
      "config":"pnpm.yml",
      "android": {
        "image": "latest"
      },
      "buildArtifactPaths": ["artifacts/**/*.png"]
    }
  },
  "submit": {
    "production": {}
  }
}
